{
  "name": "知否",
  "description" :"知否是一款 Chrome 插件，在新标签页定制化展示中文诗词",
  "version": "1.3.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icons/sakura_16.png",
      "32": "images/icons/sakura_32.png",
      "48": "images/icons/sakura_64.png",
      "128":"images/icons/sakura_128.png"
    }
  },
  "icons": {
    "16": "images/icons/sakura_16.png",
    "32": "images/icons/sakura_32.png",
    "48": "images/icons/sakura_64.png",
    "128":"images/icons/sakura_128.png"
  },
  "manifest_version": 2
}
