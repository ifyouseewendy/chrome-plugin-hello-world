{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icons/sakura_16.png",
      "32": "images/icons/sakura_32.png",
      "48": "images/icons/sakura_64.png",
      "128":"images/icons/sakura_128.png"
    }
  },
  "icons": {
    "16": "images/icons/sakura_16.png",
    "32": "images/icons/sakura_32.png",
    "48": "images/icons/sakura_64.png",
    "128":"images/icons/sakura_128.png"
  },
  "manifest_version": 2
}

